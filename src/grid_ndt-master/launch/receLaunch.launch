<launch>
<node
pkg="grid_ndt"
type="receivePcd"
name="receivePcd"
output="screen"
>
<param name="demand" type="string" value="slope" />
<param name="filename" type="string" value="stairs.pcd" />
<param name="resolution" type="double" value="0.1" />
<param name="slope_interval" type="double" value="0.0" />
<param name="z_resolution" type="double" value="0.05" />
<param name="RobotRange" type="double" value="0.35" />
<param name="reachableHeight" type="double" value="0.2" />
<param name="reachableRough" type="double" value="0.1" />
<param name="reachableAngle" type="double" value="35.0" />
<param name="is_ramp_angle" type="double" value="18.0" />

<param name="angle_size" type="int" value="48" />
<param name="curve_weight" type="double" value="1.3" />
<param name="h_weight" type="double" value="2.0" />
<param name="getup_weight" type="double" value="1.3" />
<param name="primitive_num" type="int" value="15" />
<param name="ramp_steer_num" type="int" value="7" />
<param name="rip_num" type="int" value="15" />
<param name="step_width" type="double" value="0.6" />
<param name="step_min" type="double" value="0.3" />
<param name="goal_radius" type="double" value="0.5" />
</node>

<node
name="rviz"
pkg="rviz"
type="rviz"
args="-d $(find grid_ndt)/launch/freiburg_23tri.rviz" output="log"
/>
</launch>
